// Override Bootstrap variables in this file, e.g.

font-size-base = 14px
$font-family-base = "Open Sans", "Helvetica Neue", Helvetica, Arial, sans-serif 
// NavBar Styles

$navbar-height = 85px
$navbar-margin-bottom = 0
//$grid-float-breakpoint
$navbar-border-radius = 0
//$navbar-padding-horizontal
//$navbar-padding-vertical
//$navbar-collapse-max-height
//$zindex-navbar-fixed
$navbar-default-border = 1px solid white
$navbar-default-bg = transparent
//$navbar-default-brand-color
//$navbar-default-brand-hover-color
//$navbar-default-brand-hover-bg
$navbar-default-color = #333
$navbar-default-bg = white
$navbar-default-link-color = $navbar-default-color
$navbar-default-link-hover-color = #5ABAC6
//$navbar-default-link-hover-bg
$navbar-default-link-active-color = #5ABAC6
$navbar-default-link-active-bg = white
$navbar-default-link-disabled-color = #5ABAC6
//$navbar-default-link-disabled-bg
$navbar-default-toggle-border-color = #ccc
$navbar-default-toggle-hover-bg = transparent
$navbar-default-toggle-icon-bar-bg = #ccc

// Jumbotron

$jumbotron-padding = 0
$jumbotron-heading-color = #fafafa
$jumbotron-font-size = 24px
//$jumbotron-bg
//$jumbotron-heading-font-size
$jumbotron-color = #fafafa
$jumbotron-bg-image = url('/images/city.jpg')
$jumbotron-min-height = 655px

// Footer

$thumbnail-bg = #f2f2f2
$pagination-color = #333
$pagination-hover-color = #616161
$pagination-active-color = #f2f2f2
$pagination-active-bg = #333
$pagination-active-border = #333
